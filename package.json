    "parse-gitignore": "^1.0.1",
    "picocolors": "^1.0.0"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/print-snapshots",
  "devDependencies": {
